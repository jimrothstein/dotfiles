# ~/.bash_aliases
alias goCode='cd ~/code/'
alias goDot='cd ~/dotfiles'
alias goR='cd ~/code/'
alias goBin='cd ~/bin'
export goCode goDot goR goBin

alias goScripts='cd ~/code/scripts_/'
alias goHard='cd ~/code/scripts_/vim_play/vim_hard/'

alias goBackup='cd ~/.config/nvim/backup/'

# use env var for ~/Documents
VISIBLE_DIR=~/Downloads
export VISIBLE_DIR
alias goDocs='cd $VISIBLE_DIR/documents/'

alias goLegal='cd ~/Downloads/documents/legal_18SC21468/'


